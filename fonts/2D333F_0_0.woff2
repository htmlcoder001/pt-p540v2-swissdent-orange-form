						$('.gallery-popup').fadeIn();
						
						$.post("?get_gallery_popup=" + data_id, function(data) {
							$(".gallery-popup-content").html(data);

							setTimeout(function() {
								$('.gallery-popup-box').animate( { opacity: 1 }, 1000);
